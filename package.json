{
  "name": "news-app",
  "version": "1.0.0",
  "description": "News Application",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "cd client && PORT=3001 react-scripts start",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "build": "cd client && react-scripts build"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharique-asghar/news-app.git"
  },
  "author": "Sharique Asghar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sharique-asghar/news-app/issues"
  },
  "homepage": "https://github.com/sharique-asghar/news-app#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "log4js": "^6.3.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-placeholder": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.4",
    "sass": "^1.35.1"
  },
